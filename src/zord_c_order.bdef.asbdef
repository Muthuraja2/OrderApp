projection;
strict ( 2 );
use draft;

define behavior for ZORD_C_ORDER alias Order
{
  use create;
  use update;
  use delete;

  use action Activate;
  use action Discard;
  use action Edit;
  use action Resume;
  use action Prepare;

  use association _items { create; with draft; }
}

define behavior for ZORD_C_ORDER_ITEMS alias OrderItems
{
  use update;
  use delete;

  use association _header { with draft; }
}